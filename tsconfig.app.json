{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": [
    "env.d.ts",
    "src/**/*",
    "src/**/*.vue",
    "src/**/*.vue",
    "unplugin/**/*.d.ts",
    "public/static/js/s.js",
    "public/static/js/xt3d.js"
  ],
  "exclude": ["src/**/__tests__/*"],
  "compilerOptions": {
    "types": ["node"],
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",

    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    }
  }
}
